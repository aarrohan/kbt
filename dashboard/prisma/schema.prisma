generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model DashboardUser {
  id           String    @id @default(auto()) @map("_id") @db.ObjectId
  role         String
  name         String
  email        String    @unique
  username     String    @unique
  password     String
  lastSignedIn DateTime?
  createdAt    DateTime  @default(now())
  updatedAt    DateTime  @updatedAt
}
